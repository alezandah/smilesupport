<div class="col-lg-12">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title"><i class="fa fa-money fa-fw"></i> Tickets Panel</h3>
    </div>
    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-bordered table-hover table-striped">
          <thead>
          <tr>
            <th>ID #</th>
            <th>Requester</th>
            <th>Title</th>
            <th>Assignee</th>
            <th>Status</th>
            <th>Priority</th>
          </tr>
          </thead>
          <tbody>
          <% @tickets.each do |ticket| %>
              <tr>
                <td><%= ticket.id %></td>
                <td></td>
                <td><%= ticket.title %></td>
                <td></td>
                <td></td>
                <td></td>

              </tr>
          <% end %>

          </tbody>
        </table>
      </div>
      <div class="text-right">
        <a href="#">View All Transactions <i class="fa fa-arrow-circle-right"></i></a>
      </div>
    </div>
  </div>
</div>